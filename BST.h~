#indef BST_included
#define BST_included


/* tree item */
typedef struct LLItem_s {
  struct node *root;
  char *str;
} Node;

/* tree items */
typedef struct {
  LLItem *left, *right;
} BST;

extern int llDoCheck;

/* create new list */
BST *llAlloc();


void llFree(BST *lp);


void llPut(BST *lp, char *s);


void llMakeEmpty(BST *lp);


void llPrint(BST *lp, char *msg);


int llCheck(BST *lp);
